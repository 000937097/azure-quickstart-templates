{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "existingDataFactoryName": {
      "value":"existingDF"
    },
    "existingDataFactoryResourceGroup": {
      "value":"yourDFResourceGroup"
    },
    "existingDataFactoryVersion": {
      "value":"V2"
    },
    "IntegrationRuntimeName": {
      "value":"testIR"
    },
    "NodeCount":{
      "type": "int",
      "maxValue": 4,
      "minValue": 1,
      "metadata": {
        "description": "the node count is between 1 and 4."
      }
    },
    "adminUserName": {
      "value": "GEN-USER"
    },
    "adminPassword": {
      "value": "GEN-PASSWORD"
    },
    "existingVirtualNetworkName":{
      "value": "gwvnet"
    },
    "existingVnetLocation":
    {
      "value": "West US"
    },
    "existingVnetResourceGroupName":{
      "value": "gwvnetrg"
    },
    "existingSubnetInYourVnet": {
      "value": "subnet"
    },
    "enableToSetDataStorePasswordsFromInternet": {
      "value": "yes"
    }
  }
}