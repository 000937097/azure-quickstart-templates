{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "accounts_template_name": {
            "defaultValue": "[parameters('accounts_template_name')]",
            "type": "String"
        },
        "principalId": {
            "type": "String"
        },
        "tenantId": {
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.DataShare/accounts",
            "apiVersion": "2019-11-01",
            "name": "[parameters('accounts_template_name')]",
            "location": "eastus",
            "identity": {
                "principalId": "[parameters('principalId')]",
                "tenantId": "[parameters('tenantId')]",
                "type": "SystemAssigned"
            },
            "properties": {}
        },
        {
            "type": "Microsoft.DataShare/accounts/shares",
            "apiVersion": "2019-11-01",
            "name": "[concat(parameters('accounts_template_name'), '/share')]",
            "dependsOn": [
                "[resourceId('Microsoft.DataShare/accounts', parameters('accounts_template_name'))]"
            ],
            "properties": {
                "shareKind": "CopyBased"
            }
        },
        {
            "type": "Microsoft.DataShare/accounts/shares/synchronizationSettings",
            "apiVersion": "2019-11-01",
            "name": "[concat(parameters('accounts_template_name'), '/share/share_synchronizationSetting')]",
            "dependsOn": [
                "[resourceId('Microsoft.DataShare/accounts/shares', parameters('accounts_template_name'), 'share')]",
                "[resourceId('Microsoft.DataShare/accounts', parameters('accounts_template_name'))]"
            ],
            "kind": "ScheduleBased",
            "properties": {
                "recurrenceInterval": "Day",
                "synchronizationTime": "2020-06-30T06:00:46Z"
            }
        }
    ]
}