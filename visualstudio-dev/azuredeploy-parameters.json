{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {

    "storage01Name": {
      "value": "myuniquestoragename"
      //must be globally unique name
    },
    "VMIPPublicDnsName": {
      "value": "myvmuniqueip"
      //must be globally unique name for the region you are in
      //myVMuniqueIP.westus.cloudapp.azure.com
      //It must conform to the following regular expression: ^[a-z][a-z0-9-]{1,61}[a-z0-9]$.
    },
    "storage01Type": {
      "value": "Premium_LRS"
      /*allowedValues: 
                "Premium_LRS"
                "Standard_LRS"
                "Standard_GRS"
                "Standard_ZRS"
            */
    },
    "storage01Location": {
      "value": "East US"
      /*"allowedValues": [
                "East US",
                "West US",
                "West Europe",
                "East Asia",
                "South East Asia"
            */
    },
    "Vnet01Location": {
      "value": "East US"
      /*example values, any valid option accepted
        "East US",
        "West US",
        "West Europe",
        "East Asia",
        "South East Asia"
      */
    },
    "VMName": {
      "value": "YourVSVM"
    },
    "VMAdminUserName": {
      "value": "YourAdminName"
    },
   "VMAdminPassword": {
      "value": "YourAdminPassword"
    },
    "VMSize": {
      "value": "Standard_DS2"
      /* example values, any valid option accepted but if you choose premium storage you must choose a DS class VM size
        "Standard_A3",
        "Standard_DS2",
        "Standard_A2"
      */
    },
    "VMVisualStudioVersion": {
      "value": "VS-2015-Enterprise-AzureSDK-2.7-WS2012R2"
      /*"allowedValues": 
        "VS-2015-Enterprise-AzureSDK-2.7-WS2012R2",
        "VS-2015-Community-AzureSDK-2.7-WS2012R2",
        "VS-2013-Ultimate-VSU5-AzureSDK-2.7-WS2012R2",
        "VS-2013-Community-VSU5-AzureSDK-2.7-WS2012R2"
      */
    }
  }
}
