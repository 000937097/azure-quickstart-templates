{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.1",
  "parameters": {
    "workspaceName": {
      "value": "GEN-UNIQUE-20"
    },
    "workspaceSku": {
      "value": "Standard"
    },
    "virtualNetwork": {
      "value": {
				"name": "FrontDoorApimSampleVnet",
				"addressPrefixes": ["10.0.0.0/16"],
				"subnets": [
          {
						"name": "ApiManagementSubnet",
            "addressPrefix": "10.0.0.0/24",
            "nsg": "FrontDoorApimSampleNsg"
					}
				]
			}
    },
    "appInsightsName": {
      "value": "GEN-UNIQUE-20"
    },
    "apimServiceName": {
      "value": "GEN-UNIQUE-20"
    },
    "apimPublisherEmail": {
      "value": "admin@contoso.com"
    },
    "apimPublisherName": {
      "value": "Contoso"
    },
    "apimSku": {
      "value": "Developer"
    },
    "apimCapacity": {
      "value": 1
    },
    "frontDoorName": {
      "value": "FrontDoorApimSampleAfd"
    },
    "frontDoorFrontendEndpoint": {
      "value": {
        "name": "MainFrontendEndpoint",
        "sessionAffinityEnabledState": "Disabled",
        "sessionAffinityTtlSeconds": 0
      }
    },
    "frontDoorBackendPool": {
      "value": {
        "name": "ApimBackendPool",
        "loadBalancerName": "ApimLoadBalancer"
      }
    },
    "frontDoorRoutingRule": {
      "value": {
        "name": "ApimRoutingRule",
        "acceptedProtocols": [
          "Http",
          "Https"
        ],
        "patternsToMatch": [
          "/*"
        ],
        "customForwardingPath": "/",
        "forwardingProtocol": "MatchRequest",
        "cacheConfiguration": {
          "queryParameterStripDirective": "StripNone",
          "dynamicCompression": "Enabled"
        }
      }
    },
    "frontDoorHealthProbeSettings": {
      "value": {
        "name": "ApimHealthProbeSettings",
        "intervalInSeconds": 30,
        "path": "/status-0123456789abcdef",
        "protocol": "Https"
      }
    },
    "deployWaf": {
      "value": true
    },
    "wafPolicyName": {
      "value": "FrontDoorApimSampleWaf"
    },
    "wafMode": {
      "value": "Detection"
    },
    "allowTrafficOnlyFromFrontDoor": {
      "value": false
    }
  }
}