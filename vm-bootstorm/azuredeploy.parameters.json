{
  "$schema": "http://schema.management.azure.com/schemas/2014-04-01-preview/deploymentParameters.json#",
  "contentVersion": "1.15.11.2",
  "parameters": {
    "azureAdApplicationId": {
      "value": "<Azure AD Application ID used as Username>"
    },
    "azureAdApplicationPassword": {
      "value": "<Your secured password to Azure AD Application>"
    },
    "tenantId": {
      "value": "<Your secured tenant Id from Azure Subscription>"
    },
    "dnsNameForPublicIP": {
      "value": "<Unique DNS Name e.g. vmbootdns103115>"
    },
    "storageAccountName": {
      "value": "<Unique storage name e.g. vmbootsa103115>"
    },
    "location": {
      "value": "East US 2"
    },
    "vmCount": {
      "value": 2
    },
    "vmOsSku": {
      "value": "2012-R2-Datacenter"
    },
    "vmName": {
      "value": "vmbootvm"
    },
    "vmAdminUsername": {
      "value": "vmbootadmin"
    },
    "vmAdminPassword": {
      "value": "vmbootpass!123"
    }
  }
}