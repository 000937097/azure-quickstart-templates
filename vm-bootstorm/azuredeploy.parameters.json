{
  "$schema": "http://schema.management.azure.com/schemas/2014-04-01-preview/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "storageAccountName": {
      "value": "vmbootsa"
    },
    "location": {
      "value": "East US 2"
    },
    "vmAdminUsername": {
      "value": "vmbootadmin"
    },
    "vmAdminPassword": {
      "value": "vmbootpass*123"
    },
    "vmName": {
      "value": "vmbootvm"
    },
    "vmCount": {
      "value": 2
    },
    "vmOsDiskName": {
      "value": "vmbootosdisk"
    },
    "vmDataDiskName": {
      "value": "vmbootdatadisk"
    },
    "vmDataDiskSize": {
      "value": 2
    },
    "vmOsSku": {
      "value": "2012-R2-Datacenter"
