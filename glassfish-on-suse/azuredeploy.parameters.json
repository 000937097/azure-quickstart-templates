{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "artifactsLocation": {
      "value": "https://raw.githubusercontent.com/CalCof/azure-quickstart-templates/master/glassfish-on-suse/"
    },
    "glassfishVersion": {
      "value": 3
    },
