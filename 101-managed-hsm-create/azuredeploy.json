{
	"$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
	    "location": {
          "type": "string",
          "metadata": {
            "description": "Specifies the Azure location where the managed hsm should be created."
          },
          "defaultValue": "[resourceGroup().location]"
        },
		"skuName": {
			"type": "object"
		},
		"initialAdminObjectIds": {
			"type": "array"
		},
		"managedHSMResourceName": {
			"type": "string"
		},
		"tenantId": {
			"type": "string"
		}
	},
	"resources": [{
		"type": "Microsoft.KeyVault/managedHSMs",
		"name": "[parameters('managedHSMResourceName')]",
		"location": "[parameters('location')]",
		"apiVersion": "2020-04-01-preview",
		"tags": {
			"resourceType": "MHSM",
			"Environment": "PROD"
		},
		"Sku": "[parameters('skuName')]",
		"properties": {
			"EnableSoftDelete": true,
			"SoftDeleteRetentionInDays": 90,
			"EnablePurgeProtection": false,
			"TenantId": "[parameters('tenantId')]",
			"InitialAdminObjectIds": "[parameters('initialAdminObjectIds')]"
		}
	}]
}