{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "account_name":{
            "type": "string",
            "defaultValue": "exampleMap",
            "metadata": {
                "description": "The name for your Azure Maps account."
            }
        },
        "pricingTier": {
            "type": "string",
            "allowedValues":[
                "S0",
                "S1"
            ],
            "defaultValue": "S0",
            "metadata": {
                "description": "The pricing tier for the account. Use S0 for small-scale development. Use S1 for large-scale applications."
            }
        }
    },
    "variables": {
    },
    "resources": [
        {
            "name": "[parameters('account_name')]",
            "type": "Microsoft.Maps/accounts",
            "apiVersion": "2020-02-01-preview",
            "location": "global",
            "tags": {
            },
            "sku": {
                "name": "[parameters('pricingTier')]"
            }
        }
    ],
    "outputs": {
        "primaryKey": {
            "type": "string",
            "value": "[listkeys(resourceId('Microsoft.Maps/accounts', parameters('account_name')), '2020-02-01-preview').primaryKey]"
        }
        
    }
}