{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "existingVnetName": {
      "value": "GET-PREREQ-existingVnetName"
    },
    "adUsername": {
      "value": "GET-PREREQ-existingAdminUsername",
      "metadata": {
        "description": "The value of Active Directory username"
      }
    },
    "adPassword": {
      "value": "GET-PREREQ-existingAdminPassword",
      "metadata": {
        "description": "The value of Active Directory password"
      }
    },
    "dnsServerIp": {
      "value": "GET-PREREQ-existingDNSServerIp",
      "metadata": {
        "description": "DNS server IP address"
      }
    },
    "domainFqdn": {
      "value": "GET-PREREQ-existingDomainFqdn",
      "metadata": {
        "description": "Domain name"
      }
    },
    "smbServerName": {
      "value": "pmcsmb",
      "metadata": {
        "description": "SMB server name"
      }
    },
    "anfSubnetAddressPrefix": {
      "value": "10.0.1.0/24",
      "metadata": {
        "description": "Subnet address range."
      }
    }
  }
}