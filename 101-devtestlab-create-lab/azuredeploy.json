{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "labname" : {
      "type": "string",
      "metadata": {
        "description" : "The name of the new DevTestLab instance to be created."
      }
    },
    "location" : {
      "type": "string",
      "allowedValues": [
        "East Asia"
      ],      
      "metadata": {
        "description" : "The location where the new DevTestLab instance will be created."
      }
    }
  },
  "resources": [
    {
      "apiVersion": "2015-05-21-preview",
      "type": "Microsoft.DevTestLab/labs",
      "name": "[parameters('labname')]",
      "location": "[parameters('location')]"
    }
  ]
}
