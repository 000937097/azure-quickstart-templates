{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "accounts_template_name": {
            "defaultValue": "template",
            "type": "String"
        },
        "containerName": {
            "type": "String"
        },
        "prefix": {
            "type": "String"
        },
        "subscriptionId": {
            "type": "String"
        },
        "resourceGroup": {
            "type": "String"
        },
        "storageAccountName": {
            "type": "String"
        },
        "dataSetId": {
            "type": "String"
        },
        "principalId": {
            "type": "String"
        },
        "tenantId": {
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.DataShare/accounts",
            "apiVersion": "2019-11-01",
            "name": "[parameters('accounts_template_name')]",
            "location": "eastus",
            "identity": {
                "principalId": "[parameters('principalId')]",
                "tenantId": "[parameters('tenantId')]",
                "type": "SystemAssigned"
            },
            "properties": {}
        },
        {
            "type": "Microsoft.DataShare/accounts/shares",
            "apiVersion": "2019-11-01",
            "name": "[concat(parameters('accounts_template_name'), '/share')]",
            "dependsOn": [
                "[resourceId('Microsoft.DataShare/accounts', parameters('accounts_template_name'))]"
            ],
            "properties": {
                "shareKind": "CopyBased"
            }
        },
        {
            "type": "Microsoft.DataShare/accounts/shares/dataSets",
            "apiVersion": "2019-11-01",
            "name": "[concat(parameters('accounts_template_name'), '/share/',parameters('prefix'))]",
            "dependsOn": [
                "[resourceId('Microsoft.DataShare/accounts/shares', parameters('accounts_template_name'), 'share')]",
                "[resourceId('Microsoft.DataShare/accounts', parameters('accounts_template_name'))]"
            ],
            "kind": "Container",
            "properties": {
                "containerName": "[parameters('containerName')]",
                "prefix": "[parameters('prefix')]",
                "subscriptionId": "[parameters('subscriptionId')]",
                "resourceGroup": "[parameters('resourceGroup')]",
                "storageAccountName": "[parameters('storageAccountName')]",
                "dataSetId": "[parameters('dataSetId')]"
            }
        }
    ]
}