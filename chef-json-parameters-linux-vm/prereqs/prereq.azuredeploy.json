{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vmDnsName": {
            "type": "string",
            "metadata": {
                "description": "DNS name"
            }
        }
    },
    "variables": {

    },
    "resources": [],
    "outputs": {
        "serverUrl": {
            "type": "string",
            "value": "[concat('https://',parameters('vmDnsName'),'.westus.cloudapp.net')]"
        },
        "dnsName": {
            "type": "string",
            "value": "[parameters('vmDnsName')]"
        }
    }
}