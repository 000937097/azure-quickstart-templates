{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "existingSubnetName": {
      "value": "default"
    },
    "existingVirtualNetworkName": {
      "value": "vmName-vnet"
    },
    "virtualMachineName": {
      "value": "vmName"
    },
    "virtualMachineSize": {
      "value": "Standard_DS13_v2"
    },
    "adminUsername": {
      "value": "admin"
    },
    "adminPassword": {
      "value": "password"
    },
    "storageWorkloadType": {
      "value": "GENERAL"
    },
    "diskConfigurationType": {
      "value": "NEW"
    },
    "dataDisksLUNs": {
      "value": [ 0 ]
    },
    "dataPath": {
      "value": "F:\\Data\\"
    },
    "logDisksLUNs": {
      "value": [ 1 ]
    },
    "logPath": {
      "value": "G:\\Log\\"
    },
    "tempDbPath": {
      "value": "D:\\Temp\\"
    }
  }
}